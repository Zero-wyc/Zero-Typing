{
    "name": "typing-cn",
    "description": "Chinese type test with PinYin.",
    "version": "20231127a",
    "packageManager": "pnpm@6.27.1",
    "repository": {
        "url": "https://gitee.com/barneyZhao/typing-cn.git",
        "url-github": "https://github.com/BarneyZhao/typing-cn.git"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build --mode nobase",
        "build-base": "tsc && vite build",
        "preview": "vite preview --base=/",
        "preview-base": "vite preview",
        "update-version": "node scripts/updateVersion.js",
        "online": "npm run build && node scripts/deploy.js master docs-nobase/",
        "online-base": "npm run build-base && node scripts/deploy.js master docs/",
        "lint": "eslint --ext .jsx,.js,.tsx,.ts src",
        "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
        "word": "node scripts/generateWords.js"
    },
    "dependencies": {
        "@ant-design/icons": "^4.8.3",
        "antd": "^4.24.16",
        "chinese-to-pinyin": "^1.3.1",
        "dayjs": "^1.11.13",
        "pinyin-pro": "^3.26.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.9",
        "react-router-dom": "^6.30.0",
        "redux": "^4.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.27.1",
        "@babel/plugin-syntax-flow": "^7.27.1",
        "@babel/plugin-transform-react-jsx": "^7.27.1",
        "@npmcli/package-json": "^1.0.1",
        "@types/less": "^3.0.8",
        "@types/node": "^17.0.45",
        "@types/react": "^17.0.85",
        "@types/react-dom": "^17.0.26",
        "@types/react-redux": "^7.1.34",
        "@vitejs/plugin-legacy": "^1.8.2",
        "@vitejs/plugin-react": "^1.3.2",
        "chalk": "^4.1.2",
        "eslint": "^8.57.1",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-prettier": "^4.2.1",
        "gh-pages": "^3.2.3",
        "less": "^4.3.0",
        "mustache": "^4.2.0",
        "open": "^8.4.2",
        "patch-package": "^8.0.0",
        "postcss": "^8.5.3",
        "postcss-less": "^6.0.0",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "^2.8.8",
        "rollup": "^2.79.2",
        "rollup-plugin-visualizer": "^5.14.0",
        "stylelint": "^14.16.1",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-standard": "^25.0.0",
        "typescript": "^4.9.5",
        "vite": "^2.9.18"
    }
}
